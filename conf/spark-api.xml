<rules>
    <rule>
        <regex>.*Running task.* in stage (?&lt;stageID&gt;[0-9]+\.[0-9]).*TID (?&lt;TID&gt;[0-9]+).*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <!-- required -->
            <name>stage</name>
            <type>state</type>
            <tag>stageID</tag>
            <!-- optional, default: false -->
            <isFinish>false</isFinish>
            <value>stageID</value>
        </group>
        <!--group>
            <name>task</name>
            <type>event</type>
            <tag>TID</tag>
            <isFinish>false</isFinish>
            <value>1</value>
        </group-->
    </rule>
    <rule>
        <regex>.*Finished task.*in (?&lt;stageID&gt;stage [0-9]+\.[0-9]).*TID (?&lt;TID&gt;[0-9]+).*\s(?&lt;byte&gt;[0-9]+).*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <name>stage</name>
            <type>state</type>
            <tag>stageID</tag>
            <isFinish>false</isFinish>
            <value>1</value>
        </group>
        <!--group>
            <name>task</name>
            <type>event</type>
            <tag>TID</tag>
            <isFinish>true</isFinish>
            <value>1</value>
        </group-->
        <group>
            <name>byte.transferred</name>
            <type>state</type>
            <isFinish>true</isFinish>
            <value>byte</value>
        </group>
    </rule>
    <rule>
        <regex>.*Executor killed task.*in(?&lt;stageID&gt;stage [0-9]+\.[0-9]).*TID (?&lt;TID&gt;[0-9]+).*</regex>
        <dateOffset>-7200000</dateOffset>
        <!--group>
            <name>task</name>
            <type>event</type>
            <tag>TID</tag>
            <isFinish>true</isFinish>
            <value>1</value>
        </group-->
    </rule>
    <rule>
        <regex>.*stored as (?:values|bytes) in memory.*estimated size (?&lt;byte&gt;[0-9]+\.[0-9]+.*),.*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <name>byte.stored</name>
            <type>state</type>
            <isFinish>true</isFinish>
            <value>byte</value>
        </group>
    </rule>
    <rule>
        <regex>.*Task (?&lt;TID&gt;[0-9]+) force spilling in-memory map to disk.*(?&lt;byte&gt;[0-9]+\.[0-9]+.*) memory</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <name>byte.spill</name>
            <type>state</type>
            <tag>TID</tag>
            <isFinish>true</isFinish>
            <value>byte</value>
        </group>
    </rule>
    <rule>
        <regex>.*spilling in-memory map of (?&lt;byte&gt;[0-9]+\.[0-9]+.*) to disk.*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <name>byte.spill</name>
            <type>state</type>
            <isFinish>false</isFinish>
            <value>byte</value>
        </group>
    </rule>
    <rule>
        <regex>.*have map outputs for shuffle.*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <name>shuffle</name>
            <type>event</type>
            <isFinish>false</isFinish>
            <value>1.0</value>
        </group>
    </rule>
    <rule>
        <regex>.*Started.*remote fetches in.*</regex>
        <dateOffset>-7200000</dateOffset>
        <group>
            <name>shuffle</name>
            <type>event</type>
            <isFinish>true</isFinish>
            <value>1.0</value>
        </group>
    </rule>
</rules>